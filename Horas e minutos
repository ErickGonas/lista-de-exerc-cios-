import java.util.*;

public class Exercicios_4 {
    public static void main (String[] args){
        int hora,minuto,minpassados;
     Scanner ler= new
            Scanner(System.in);
     
     System.out.println("Informe o valor da hora (HH:MM) ");
        hora= ler.nextInt();
        minuto= ler.nextInt();
       
    minpassados=(hora*60)+minutos;
                 
     System.out.println("minutos que se passaram:);      
    }
    
